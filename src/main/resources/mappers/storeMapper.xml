<?xml version="1.0" encoding="UTF-8"?>


  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.ghfkd1235.spring.ex.database.dao.StoreDAO">
  
  	<select id="selectStoreList" resultType="com.ghfkd1235.spring.ex.database.model.Store">
  		
  		SELECT
  			`id`
  			, `name`
  			, `phoneNumber`
  			, `address`
  			, `businessNumber`
  			, `introduce`
  			, `createdAt`
  			, `updatedAt`
  		FROM
  			`store`;
  			
  	</select>
  
  </mapper>